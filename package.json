{
  "name": "mharj-jwt-middleware",
  "version": "0.0.4",
  "description": "JWT ExpressJS middleware",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "mocha": {
    "exit": true,
    "extension": [
      "ts",
      "js"
    ],
    "recursive": true,
    "enable-source-maps": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporters": [
      "spec",
      "mocha-junit-reporter"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "lcovonly"
    ],
    "all": true,
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mharj/mharj-jwt-middleware.git"
  },
  "keywords": [
    "jwt",
    "jsonwebtoken",
    "openid",
    "util",
    "aad"
  ],
  "author": "mharj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mharj/mharj-jwt-middleware/issues"
  },
  "homepage": "https://github.com/mharj/mharj-jwt-middleware#readme",
  "devDependencies": {
    "@azure/identity": "^2.0.4",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/sinon": "^10.0.11",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "mharj-jwt-util": "^0.2.3",
    "typed-emitter": "^2.1.0"
  },
  "peerDependencies": {
    "express": "^4.x"
  }
}
