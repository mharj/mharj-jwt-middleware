{
  "name": "mharj-jwt-middleware",
  "version": "0.0.7",
  "description": "JWT ExpressJS middleware",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "mocha": {
    "exit": true,
    "extension": [
      "ts",
      "js"
    ],
    "recursive": true,
    "enable-source-maps": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "reporters": [
      "spec",
      "mocha-junit-reporter"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "lcovonly"
    ],
    "all": true,
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mharj/mharj-jwt-middleware.git"
  },
  "keywords": [
    "jwt",
    "jsonwebtoken",
    "openid",
    "util",
    "aad"
  ],
  "author": "mharj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mharj/mharj-jwt-middleware/issues"
  },
  "homepage": "https://github.com/mharj/mharj-jwt-middleware#readme",
  "devDependencies": {
    "@azure/identity": "^4.0.1",
    "@azure/identity-vscode": "^1.0.0",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.11",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.23.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.4",
    "sinon": "^17.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "mharj-jwt-util": "^0.5.1",
    "typed-emitter": "^2.1.0"
  },
  "peerDependencies": {
    "express": "^4.x"
  }
}
